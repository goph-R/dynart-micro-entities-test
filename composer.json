{
    "name": "dynart/micro-entities-test",
    "description": "Test suite for dynart-micro-entities",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "dynart/micro-entities": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Dynart\\Micro\\Entities\\Test\\": "src/"
        }
    },
    "repositories": {
        "dynart-micro-entities": {
            "type": "path",
            "url": "../dynart-micro-entities",
            "options": {
                "symlink": true
            }
        },
        "dynart-micro": {
            "type": "path",
            "url": "../dynart-micro",
            "options": {
                "symlink": true
            }
        }
    },
    "minimum-stability": "dev"
}
